<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
"-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
"http://struts.apache.org/dtds/struts-2.3.dtd">
<struts>
	<constant name="struts.ui.theme" value="simple" />
	<!-- 開発者モード(デバッグしたい時にはコメント外してください。ユーザーのエラーページが無効にできます) -->
	<!-- <constant name="struts.devMode" value="true" /> -->
	<constant name="struts.devMode" value="true" />
	<package name="com.took.action" extends="struts-default">

	<action name="SigninConfirmAction" class="com.took.action.SigninConfirmAction" method="execute">
		<result name="success">signinConfirm.jsp</result>
		<result name="error">signin.jsp</result>
	</action>
	<action name="SigninCompleteAction" class="com.took.action.SigninCompleteAction" method="execute">
		<result name="success">index.jsp</result>
		<result name="back">signin.jsp</result>
		<result name="home">index.jsp</result>
		<result name="error">signinConfirm.jsp</result>
	</action>

<!-- 	<action name="LoginAction" class="com.took.action.LoginAction" method="execute">
		<result name="success">home.jsp</result>
		<result name="error">userCreateConfirm.jsp</result>
	</action> -->


</package>
</struts>