<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
"-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
"http://struts.apache.org/dtds/struts-2.3.dtd">
<struts>
	<constant name="struts.ui.theme" value="simple" />
	<!-- 開発者モード(デバッグしたい時にはコメント外してください。ユーザーのエラーページが無効にできます) -->
	<!-- <constant name="struts.devMode" value="true" /> -->
	<constant name="struts.devMode" value="true" />
	<package name="com.took.action" extends="struts-default">

	<action name="UserCreateConfirmAction" class="com.took.action.UserCreateConfirmAction" method="execute">
		<result name="success">userCreateConfirm.jsp</result>
		<result name="error">signin.jsp</result>
	</action>
	<action name="UserCreateCompleteAction" class="com.took.action.UserCreateCompleteAction" method="execute">
		<result name="success">home.jsp</result>

	</action>



</package>
</struts>